<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" namespace="/" extends="ironrhino-default">
		<action name="product" class="com.ironrhino.online.action.ProductAction">
			<result name="input" type="redirect">/product/view/${uid}</result>
			<result name="success" type="redirect">/product/view/${uid}</result>
			<result name="list" type="autoconfig"/>
			<result name="view" type="autoconfig"/>
		</action>
	</package>
</struts>