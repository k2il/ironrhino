<?xml version="1.0" encoding="UTF-8"?>
<beans default-autowire="byName"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="waterMark" class="org.ironrhino.common.util.WaterMark"
		p:mark="/images/logo.gif" />
	<bean id="smallThumbnail"
		class="org.ironrhino.common.util.Thumbnail" p:height="100"
		p:width="100" />
	<bean id="mediumThumbnail"
		class="org.ironrhino.common.util.Thumbnail" p:height="200"
		p:width="200" />
	<bean id="categoryTreeControl"
		class="org.ironrhino.pms.support.CategoryTreeControl" />
	<bean id="categoryManager"
		class="org.ironrhino.pms.service.CategoryManagerImpl" />
	<bean id="productManager"
		class="org.ironrhino.pms.service.ProductManagerImpl" />
</beans>
