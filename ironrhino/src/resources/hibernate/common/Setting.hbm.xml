<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.common.model"
	default-lazy="true">
	<class name="Setting" table="common_setting">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="key" column="skey" not-null="true" />
		<property name="value" />
		<property name="createDate" />
		<property name="modifyDate" />
		<many-to-one name="createUser" class="org.ironrhino.security.model.User" column="createUser" not-null="false" lazy="proxy" />
		<many-to-one name="modifyUser" class="org.ironrhino.security.model.User" column="modifyUser" not-null="false" lazy="proxy" />
	</class>
</hibernate-mapping>