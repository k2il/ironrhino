<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.common.model"
	default-lazy="true">
	<class name="Setting" table="common_setting">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<natural-id mutable="true">
			<property name="key" column="skey" not-null="true" />
		</natural-id>
		<property name="value" />
		<property name="description" />
		<property name="createDate" />
		<property name="modifyDate" />
		<property name="createUserAsString" column="createUser" />
		<property name="modifyUserAsString" column="modifyUser" />
	</class>
</hibernate-mapping>