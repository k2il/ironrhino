<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.common.model"
	default-lazy="true">
	<class name="Schema" table="common_schema">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<natural-id mutable="true">
			<property name="name" not-null="true" />
		</natural-id>
		<property name="description" />
		<property name="strict" />
		<property name="fieldsAsString" column="fields" length="1024"/>
	</class>
</hibernate-mapping>