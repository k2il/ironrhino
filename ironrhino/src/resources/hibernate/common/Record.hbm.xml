<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.common.model"
	default-lazy="true">
	<class name="Record" table="common_record">
		<id name="id" type="java.lang.String" 
			unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="operatorId" />
		<property name="operatorClass" />
		<property name="entityId" />
		<property name="entityClass" />
		<property name="entityToString" />
		<property name="action" />
		<property name="recordDate" />
	</class>
</hibernate-mapping>