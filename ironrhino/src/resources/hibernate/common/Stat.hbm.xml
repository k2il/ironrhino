<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.common.model"
	default-lazy="true">
	<class name="Stat" table="common_stat">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="keyAsString" not-null="true" />
		<property name="valueAsString" not-null="true" />
		<property name="date" not-null="true"/>
		<property name="host" not-null="true"/>
	</class>
</hibernate-mapping>