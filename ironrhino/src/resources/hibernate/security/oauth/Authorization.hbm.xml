<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.security.oauth.server.model"
	default-lazy="true">
	<class name="Authorization" table="oauth_authorization">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="uuid" />
		</id>
		<natural-id>
			<property name="accessToken" not-null="true" />
		</natural-id>
		<property name="code" unique="true" />
		<property name="scope" />
		<property name="lifetime" />
		<property name="refreshToken" unique="true" />
		<property name="responseType" not-null="true" />
		<property name="modifyDate" not-null="true" />
		<property name="createDate" not-null="true" />
		<many-to-one name="grantor" lazy="false" fetch="select" />
		<many-to-one name="client" lazy="false" fetch="select" />
	</class>
</hibernate-mapping>