<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.ums.model"
	default-lazy="true">
	<class name="User" table="ums_user">
		<id name="id" type="java.lang.String" length="32"
			unsaved-value="null">
			<generator class="uuid" />
		</id>
		<natural-id>
			<property name="username" not-null="true" />
		</natural-id>
		<property name="password" not-null="true" />
		<property name="name" />
		<property name="email" />
		<property name="description" />
		<property name="accountExpireDate" />
		<property name="locked" />
		<property name="passwordExpireDate" />
		<property name="enabled" />
		<property name="createDate" />
		<property name="modifyDate" />
		<set name="roles" table="ums_userRole" cascade="all"
			fetch="subselect">
			<key column="userId" />
			<composite-element
				class="org.ironrhino.common.model.SimpleElement">
				<property name="value" not-null="true" />
			</composite-element>
		</set>
		<set name="groups" table="ums_userGroup" fetch="subselect"
			cascade="all">
			<key column="userId" />
			<composite-element
				class="org.ironrhino.common.model.SimpleElement">
				<property name="value" not-null="true" />
			</composite-element>
		</set>
	</class>
</hibernate-mapping>