<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ironrhino.pms.model"
	default-lazy="true">
	<class name="Category" table="pms_category">
		<id name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="identity" />
		</id>
		<natural-id>
			<property name="code" not-null="true" />
		</natural-id>
		<property name="name" not-null="true" />
		<property name="description" />
		<property name="level" />
		<property name="displayOrder" />
		<property name="fullId" />
		<many-to-one name="parent" class="Category" column="parentId" />
		<set name="children" lazy="true" inverse="true" cascade="none"
			order-by="displayOrder,name">
			<key column="parentId" />
			<one-to-many class="Category" />
		</set>
		<set name="roles" table="pms_categoryRole" cascade="all"
			fetch="subselect">
			<key column="categoryId" />
			<composite-element
				class="org.ironrhino.common.model.SimpleElement">
				<property name="value" not-null="true" />
			</composite-element>
		</set>
	</class>
</hibernate-mapping>