<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.online.model"
	default-lazy="true">
	<class name="ProductScore" table="online_productScore">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<natural-id>
			<property name="username" length="150" not-null="true" />
			<property name="productCode" length="150" not-null="true" />
		</natural-id>
		<property name="score" />
		<property name="scoreDate" />
	</class>
</hibernate-mapping>