<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.online.model"
	default-lazy="true">
	<class name="Feedback" table="online_feedback">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="uuid" />
		</id>
		<property name="username" />
		<property name="name" />
		<property name="email" />
		<property name="phone" />
		<property name="title" />
		<property name="content" />
		<property name="status">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">
					com.ironrhino.online.model.FeedbackStatus
				</param>
				<!-- if set type=12 then use name not ordinal
					<param name="type">12</param>
					-->
			</type>
		</property>
		<property name="createDate" />
		<property name="modifyDate" />
	</class>
</hibernate-mapping>