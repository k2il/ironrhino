<?xml version="1.0" encoding="UTF-8"?>
<beans default-autowire="byName"
	xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean name="/hessian/*" class="org.ironrhino.core.remoting.server.HessianServer" />
	<bean name="/httpinvoker/*" class="org.ironrhino.core.remoting.server.HttpInvokerServer" />
	<bean name="/jsoncall/*/*" class="org.ironrhino.core.remoting.server.JsonCallServer" />
</beans>