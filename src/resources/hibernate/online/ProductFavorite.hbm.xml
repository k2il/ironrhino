<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.online.model"
	default-lazy="true">
	<class name="ProductFavorite" table="online_productFavorite">
		<id name="id" type="java.lang.String" length="32"
			unsaved-value="null">
			<generator class="uuid" />
		</id>
		<natural-id>
			<property name="username" length="150" not-null="true" />
			<property name="productCode" length="150" not-null="true" />
		</natural-id>
		<property name="productName" />
		<property name="addDate" />
	</class>
</hibernate-mapping>