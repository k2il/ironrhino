<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ironrhino.online.model"
	default-lazy="true">
	<class name="Announcement" table="online_announcement">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="title" />
		<property name="content" type="text" />
		<property name="released" />
		<property name="sticky" />
		<property name="releaseDate" />
	</class>
</hibernate-mapping>