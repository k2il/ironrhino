<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.daq.model"
	default-lazy="true">
	<class name="Acquisition" table="acquisition">
		<id name="id" type="java.lang.String" length="32" unsaved-value="null">
			<generator class="uuid" />
		</id>
		<property name="type" length="50" index="type_place" />
		<property name="place" length="50" index="type_place" />
		<property name="time" />
		<property name="value" />
		<property name="ip" />
	</class>
</hibernate-mapping>