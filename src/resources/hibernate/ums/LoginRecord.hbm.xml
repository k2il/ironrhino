<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.ironrhino.ums.model">
	<class name="LoginRecord" table="ums_loginRecord">
		<id name="id" type="java.lang.String" 
			length="32" unsaved-value="null">
			<generator class="org.ironrhino.core.hibernate.StringIdGenerator" />
		</id>
		<property name="username" />
		<property name="loginDate" />
		<property name="loginAddress" />
		<property name="failed" />
		<property name="cause" />
	</class>
</hibernate-mapping>